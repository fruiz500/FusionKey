{
  "name": "FusionKey",
  "short_name": "FusionKey",
  "description": "High-security passwords and encryption", 
  "omnibox": { "keyword" : "src" },
  "version": "0.1.10",
  "manifest_version": 3,
  "icons": {
  "16": "images/icon16.png",
  "48": "images/icon48.png",
  "128": "images/icon128.png"
  },
  "permissions": [
    "storage", "activeTab",
    "scripting", "alarms"
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; img-src 'self' data:; connect-src 'self' data:; style-src 'self' 'unsafe-inline'; frame-src 'self' https://*.passlok.com https://passlok.com"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
        "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
        },
        "default_title": "FusionKey",
        "default_popup": "index.html"
  },
  "commands": {
  		"_execute_browser_action": {
            "suggested_key": {
              "windows": "Ctrl+Shift+K",
              "mac": "Command+Shift+K",
              "chromeos": "Ctrl+Shift+K",
              "linux": "Ctrl+Shift+K"
			}
  		}
  },
  "browser_specific_settings": {
      "gecko": {
        "id": "fusionkey@passlok.com"
      }
    }
}